{"ast":null,"code":"import TableData from './TableData.vue';\nimport axios from 'axios';\nexport default {\n  components: {\n    TableData\n  },\n  data() {\n    return {\n      users: []\n    };\n  },\n  created() {\n    this.fetchUsers();\n  },\n  methods: {\n    async fetchUsers() {\n      try {\n        const response = await axios.get('https://65f72198b4f842e8088528a5.mockapi.io/users');\n        this.users = response.data;\n      } catch (error) {\n        console.error('Error fetching users:', error);\n      }\n    },\n    async editUser(user) {\n      try {\n        await axios.put(`https://65f72198b4f842e8088528a5.mockapi.io/users${user.id}`, user);\n        console.log('User updated:', user);\n      } catch (error) {\n        console.error('Error updating user:', error);\n      }\n    },\n    async deleteUser(userId) {\n      try {\n        await axios.delete(`https://65f72198b4f842e8088528a5.mockapi.io/users/${userId}`);\n        this.users = this.users.filter(user => user.id !== userId);\n        console.log('User deleted with ID:', userId);\n      } catch (error) {\n        console.error('Error deleting user:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["TableData","axios","components","data","users","created","fetchUsers","methods","response","get","error","console","editUser","user","put","id","log","deleteUser","userId","delete","filter"],"sources":["C:\\Users\\nitis\\vuecrud\\src\\components\\ParentComponent.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <button @click=\"handleButtonClick\" class=\"create-button\"> + Create Invoice</button>\r\n      <TableData :users=\"users\" @edit=\"editUser\" @delete=\"deleteUser\" />\r\n      \r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import TableData from './TableData.vue';\r\n  import axios from 'axios';\r\n  \r\n  export default {\r\n    components: {\r\n      TableData,\r\n    },\r\n    data() {\r\n      return {\r\n        users: [],\r\n      };\r\n    },\r\n    created() {\r\n      this.fetchUsers();\r\n    },\r\n    methods: {\r\n      async fetchUsers() {\r\n        try {\r\n          const response = await axios.get('https://65f72198b4f842e8088528a5.mockapi.io/users');\r\n          this.users = response.data;\r\n        } catch (error) {\r\n          console.error('Error fetching users:', error);\r\n        }\r\n      },\r\n      async editUser(user) {\r\n        try {\r\n          await axios.put(`https://65f72198b4f842e8088528a5.mockapi.io/users${user.id}`, user);\r\n          console.log('User updated:', user);\r\n        } catch (error) {\r\n          console.error('Error updating user:', error);\r\n        }\r\n      },\r\n      async deleteUser(userId) {\r\n        try {\r\n          await axios.delete(`https://65f72198b4f842e8088528a5.mockapi.io/users/${userId}`);\r\n          this.users = this.users.filter(user => user.id !== userId);\r\n          console.log('User deleted with ID:', userId);\r\n        } catch (error) {\r\n          console.error('Error deleting user:', error);\r\n        }\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n\r\n<style>\r\n\r\n.create-button {\r\n    background-color: blue; \r\n    color:white;\r\n    border-radius: none;\r\n    margin: 20px;\r\n    padding : 10px;\r\n    font-size: 15px;\r\n\r\n  }\r\n  </style>\r\n  "],"mappings":"AASE,OAAOA,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMP,KAAK,CAACQ,GAAG,CAAC,mDAAmD,CAAC;QACrF,IAAI,CAACL,KAAI,GAAII,QAAQ,CAACL,IAAI;MAC5B,EAAE,OAAOO,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C;IACF,CAAC;IACD,MAAME,QAAQA,CAACC,IAAI,EAAE;MACnB,IAAI;QACF,MAAMZ,KAAK,CAACa,GAAG,CAAE,oDAAmDD,IAAI,CAACE,EAAG,EAAC,EAAEF,IAAI,CAAC;QACpFF,OAAO,CAACK,GAAG,CAAC,eAAe,EAAEH,IAAI,CAAC;MACpC,EAA
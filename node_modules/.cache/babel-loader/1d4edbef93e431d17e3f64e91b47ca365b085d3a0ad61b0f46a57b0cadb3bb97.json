:', user);\r\n    },\r\n    deleteUser(userId) {\r\n      // Handle delete logic here\r\n      console.log('Deleting user with ID:', userId);\r\n      this.users = this.users.filter(user => user.id !== userId);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\ntable {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n}\r\n\r\nth, td {\r\n  padding: 8px;\r\n  text-align: left;\r\n  border-bottom: 1px solid #ddd;\r\n}\r\n\r\nth {\r\n  background-color: #f2f2f2;\r\n}\r\n\r\nbutton {\r\n  margin-right: 5px;\r\n}\r\n</style>\r\n"],"mappings":"AA2BA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,UAAU;QAAEC,KAAK,EAAE;MAAmB,CAAC,EACtD;QAAEF,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,YAAY;QAAEC,KAAK,EAAE;MAAmB,CAAC,EACxD;QAAEF,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,eAAe;QAAEC,KAAK,EAAE;MAAoB,CAAC;IAEhE,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAACC,IAAI,EAAE;MACb;MACAC,OAAO,CAACC,GAAG,CAAC,eAAe,EAAEF,IAAI,CAAC;IACpC,CAAC;IACDG,UAAUA,CAACC,MAAM,EAAE;MACjB;MACAH,OAAO,CAACC,GAAG,CAAC,wBAAwB,EAAEE,MAAM,CAAC;MAC7C,IAAI,CAACV,KAAI,GAAI,IAAI,CAACA,KAAK,CAACW,MAAM,CAACL,IAAG,IAAKA,IAAI,CAACL,EAAC,KAAMS,MAAM,CAAC;IAC5D;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
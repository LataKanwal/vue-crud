          }\r\n        })\r\n        .catch(error => {\r\n          console.error('Error updating item:', error);\r\n        });\r\n    },\r\n    deleteItem(id) {\r\n      axios.delete(`https://65f72198b4f842e8088528a5.mockapi.io/users/${id}`)\r\n        .then(() => {\r\n          this.items = this.items.filter(item => item.id !== id);\r\n        })\r\n        .catch(error => {\r\n          console.error('Error deleting item:', error);\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.top-left-button {\r\n  position: absolute;\r\n  top: 10px;\r\n  left: 10px;\r\n  padding: 10px 20px;\r\n  background-color: rgb(139, 45, 183);\r\n  color:white;\r\n  border:none;\r\n  border-radius:5px;\r\n  text-decoration: none;\r\n  font-size: 18px;\r\n  margin: 20px 3px;\r\n  cursor: pointer;\r\n}\r\n\r\n</style>"],"mappings":";AASA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,SAAQ,MAAO,iBAAiB;AAEvC,eAAe;EACbC,UAAU,EAAE;IACV;IACAD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,SAAS,CAAC,CAAC;EAClB,CAAC;EACDC,OAAO,EAAE;IACPD,SAASA,CAAA,EAAG;MACVN,KAAK,CAACQ,GAAG,CAAC,mDAAmD,EAC1DC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACN,KAAI,GAAIM,QAAQ,CAACP,IAAI;MAC5B,CAAC,EACAQ,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;MAC9C,CAAC,CAAC;IACN,CAAC;IACDE,OAAOA,CAAA,EAAG;MACR,MAAMC,OAAM,GAAI;QACdC,EAAE,EAAE,KAAK;QACTC,MAAM,EAAE,EAAE;QACVC,MAAM,EAAE,KAAK;QACbC,KAAK,EAAE,GAAG;QACVC,WAAW,EAAE,YAAY;QACzBC,OAAO,EAAE,GAAG;QACZC,OAAO,EAAE;MACX,CAAC;MAEDtB,KAAK,CAACuB,IAAI,CAAC,mDAAmD,EAAER,OAAO,EACpEN,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACN,KAAK,CAACoB,IAAI,CAACd,QAAQ,CAACP,IAAI,CAAC;MAChC,CAAC,EACAQ,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,oBAAoB,EAAEA,KAAK,CAAC;MAC5C,CAAC,CAAC;IACN,CAAC;IACDa,UAAUA,CAACC,WAAW,EAAE;MACtB1B,KAAK,CAAC2B,GAAG,CAAE,qDAAoDD,WAAW,CAACE,EAAG,EAAC,EAAEF,WAAW,EACzFjB,IAAI,CAAC,MAAM;QACV,MAAMoB,KAAI,GAAI,IAAI,CAACzB,KAAK,CAAC0B,SAAS,CAACC,IAAG,IAAKA,IAAI,CAACH,EAAC,KAAMF,WAAW,CA
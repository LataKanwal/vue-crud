{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport AddData from './AddData.vue';\nimport TableData from './TableData.vue';\nimport axios from 'axios';\nimport { API_ENDPOINT } from './apiConfig.js';\nexport default {\n  components: {\n    TableData,\n    AddData\n  },\n  data() {\n    return {\n      users: []\n    };\n  },\n  created() {\n    this.fetchUsers();\n  },\n  methods: {\n    async fetchUsers() {\n      try {\n        const response = await axios.get(API_ENDPOINT);\n        this.users = response.data;\n      } catch (error) {\n        console.error('Error fetching users:', error);\n      }\n    },\n    async addUser(newUser) {\n      try {\n        const response = await axios.post(API_ENDPOINT, newUser);\n        this.users.push(response.data);\n      } catch (error) {\n        console.error('Error adding user:', error);\n      }\n    },\n    async deleteUser(userId) {\n      try {\n        await axios.delete(`${API_ENDPOINT}/${userId}`);\n        this.users = this.users.filter(user => user.id !== userId);\n      } catch (error) {\n        console.error('Error deleting user:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["AddData","TableData","axios","API_ENDPOINT","components","data","users","created","fetchUsers","methods","response","get","error","console","addUser","newUser","post","push","deleteUser","userId","delete","filter","user","id"],"sources":["C:\\Users\\nitis\\vuecrud\\src\\components\\ParentComponent.vue"],"sourcesContent":["<template>\r\n  <div class=\"card\">\r\n    <div>\r\n      <AddData @add=\"addUser\"/>\r\n      <TableData :users=\"users\" @delete=\"deleteUser\"  @view=\"viewUser\" />\r\n    </div>\r\n  </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import AddData from './AddData.vue';\r\n  import TableData from './TableData.vue';\r\n  import axios from 'axios';\r\n  import { API_ENDPOINT } from './apiConfig.js';\r\n\r\n  export default {\r\n    components: {\r\n      TableData,\r\n      AddD
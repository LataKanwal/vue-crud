n      showUpdateForm(user) {\r\n      this.selectedUser = user;\r\n      this.showUpdate = true;\r\n    },\r\n    updateUser(updatedUser) {\r\n      const index = this.users.findIndex(user => user.id === updatedUser.id);\r\n      if (index !== -1) {\r\n        this.users.splice(index, 1, updatedUser);\r\n      }\r\n      this.showUpdate = false;\r\n    }\r\n      \r\n    },\r\n  };\r\n  </script>\r\n\r\n<style>\r\n.page-container {\r\n  display:flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.create-button {\r\n    background-color: rgb(169, 59, 233); \r\n    color:white;\r\n    padding : 10px;\r\n    font-size: 18px;\r\n    margin: 20px;\r\n    border:none;\r\n    text-decoration: none;\r\n    border-radius: 8px;\r\n\r\n  }\r\n  .add {\r\n  margin-bottom: 20px;\r\n  }\r\n  .card {\r\n  /* Add shadows to create the \"card\" effect */\r\n  box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2);\r\n  transition: 0.3s;\r\n}\r\n  </style>\r\n  "],"mappings":";AAWE,OAAOA,OAAM,MAAO,eAAe;AACnC,OAAOC,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,UAAS,MAAO,kBAAkB;AACzC,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,YAAW,QAAS,gBAAgB;AAG7C,eAAe;EACbC,UAAU,EAAE;IACVJ,SAAS;IACTD,OAAO;IACPE;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,UAAU,EAAE,KAAK;MACnBC,YAAY,EAAE;IAEd,CAAC;EACH,CAAC;EAEDC,OAAO,EACP;IACE,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMT,KAAK,CAACU,GAAG,CAACT,YAAY,CAAC;QAC9C,IAAI,CAACG,KAAI,GAAIK,QAAQ,CAACN,IAAI;MAC5B,EAAE,OAAOQ,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C;IACF,CAAC;IACD,MAAME,OAAOA,CAACC,OAAO,EAAE;MAC3B,IAAI;QACF,MAAML,QAAO,GAAI,MAAMT,KAAK,CAACe,IAAI,CAACd,YAAY,EAAEa,OAAO,CAAC;QACxD,IAAI,CAACV,KAAK,CAACY,IAAI,CAACP,QAAQ,CAACN,IAAI,CAAC,EAAE;QAChCS,OAAO,CAACK,GAAG,CAAC,aAAa,EAAER,QAAQ,CAACN,IAAI,CAAC;MAC3C,EAAE,OAAOQ,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,oBAAoB,EAAEA,KAAK,CAAC;MAC9C;IACI,CAAC;IAEC,MAAMO,UAAUA,CAACC,MAAM,EAAE;MACvB,IAAI;QACF,MAAMnB,KAAK,CAACoB,MAAM,CAAE,GAAEnB,YAAa,IAAGkB,MAAO,EAAC,CAAC;QAC/C,IAA
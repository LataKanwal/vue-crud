{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport AddData from './AddData.vue';\nimport TableData from './TableData.vue';\nimport axios from 'axios';\nexport default {\n  components: {\n    TableData,\n    AddData\n  },\n  data() {\n    return {\n      users: []\n    };\n  },\n  created() {\n    this.fetchUsers();\n  },\n  computed: {\n    transformedUsers() {\n      return this.users.map(user => ({\n        ...user,\n        status: user.status ? 'Delivered' : 'Pending'\n      }));\n    },\n    methods: {\n      async fetchUsers() {\n        try {\n          const response = await axios.get('https://65f72198b4f842e8088528a5.mockapi.io/users');\n          this.users = response.data;\n        } catch (error) {\n          console.error('Error fetching users:', error);\n        }\n      },\n      async addUser(newUser) {\n        this.users.push({\n          id: this.users.length + 1,\n          ...newUser\n        });\n      },\n      async deleteUser(userId) {\n        try {\n          await axios.delete(`https://65f72198b4f842e8088528a5.mockapi.io/users/${userId}`);\n          this.users = this.users.filter(user => user.id !== userId);\n          console.log('User deleted with ID:', userId);\n        } catch (error) {\n          console.error('Error deleting user:', error);\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["AddData","TableData","axios","components","data","users","created","fetchUsers","computed","transformedUsers","map","user","status","methods","response","get","error","console","addUser","newUser","push","id","length","deleteUser","userId","delete","filter","log"],"sources":["C:\\Users\\nitis\\vuecrud\\src\\components\\ParentComponent.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <TableData :users=\"transformedusers\" @delete=\"deleteUser\" />\r\n      <AddData @add=\"addUser\"/>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import AddData from './AddData.vue';\r\n  import TableData from
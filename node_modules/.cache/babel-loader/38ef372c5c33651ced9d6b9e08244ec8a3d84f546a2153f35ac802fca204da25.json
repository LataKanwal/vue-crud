{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport AddData from './AddData.vue';\nimport TableData from './TableData.vue';\nimport UpdateData from './UpdateData.vue';\nimport UserDetailsModal from './UserDetailsModal.vue'; // Import the modal component\nimport axios from 'axios';\nimport { API_ENDPOINT } from './apiConfig.js';\nexport default {\n  components: {\n    TableData,\n    AddData,\n    UpdateData,\n    UserDetailsModal\n  },\n  data() {\n    return {\n      selectedUser: null,\n      users: [],\n      selectedUserId: null,\n      showUserDetails: false // Flag to show/hide user details modal\n    };\n  },\n  created() {\n    this.fetchUsers();\n  },\n  methods: {\n    async fetchUsers() {\n      try {\n        const response = await axios.get(API_ENDPOINT);\n        this.users = response.data;\n      } catch (error) {\n        console.error('Error fetching users:', error);\n      }\n    },\n    async addUser(newUser) {\n      try {\n        const response = await axios.post(API_ENDPOINT, newUser);\n        this.users.push(response.data);\n        console.log('User added:', response.data);\n      } catch (error) {\n        console.error('Error adding user:', error);\n      }\n    },\n    async deleteUser(userId) {\n      try {\n        await axios.delete(`${API_ENDPOINT}/${userId}`);\n        this.users = this.users.filter(user => user.id !== userId);\n        console.log('User deleted with ID:', userId);\n      } catch (error) {\n        console.error('Error deleting user:', error);\n      }\n    },\n    editUser(userId) {\n      console.log('Editing user with ID:', userId);\n      this.selectedUserId = userId;\n      this.selectedUser = this.users.find(user => user.id === userId);\n    },\n    async updateUser(updatedUser) {\n      try {\n        const response = await axios.put(`${API_ENDPOINT}/${updatedUser.id}`, updatedUser);\n        const index = this.users.findIndex(user => user.id === updatedUser.id);\n        if (index !== -1) {\n          this.$set(this.users, index, response.data);\n        }\n        console.log('User updated:', response.data);\n      } catch (error) {\n        console.error('Error updating user:', error);\n      } finally {\n        this.fetchUsers();\n      }\n    },\n    viewUser(userId) {\n      console.log('Viewing user with ID:', userId);\n      this.selectedUser = this.users.find(user => user.id === userId);\n      this.showUserDetails = true; // Set the flag to true to show the user details modal\n    },\n    closeUserDetails() {\n      this.showUserDetails = false; // Set the flag to false to hide the user details modal\n    }\n  }\n};","map":{"version":3,"names":["AddData","TableData","UpdateData","UserDetailsModal","axios","API_ENDPOINT","components","data","selectedUser","users","selectedUserId","showUserDetails","created","fetchUsers","methods","response","get","error","console","addUser","newUser","post","push","log","deleteUser","userId","delete","filter","user","id","editUser","find","updateUser","updatedUser","put","index","findIndex","$set","viewUser","closeUserDetails"],"sources":["C:\\Users\\nitis\\vuecrud\\src\\components\\ParentComponent.vue"],"sourcesContent":["<template>\r\n  <div class=\"card\">\r\n    <div>\r\n      <AddData @add=\"addUser\"/>\r\n      <TableData :users=\"users\" @delete=\"deleteUser\"  @edit=\"editUser\" @view=\"viewUser\" />\r\n      <UpdateData v-if=\"selectedUserId\" :user=\"selectedUser\" @update=\"updateUser\" />\r\n      <UserDetailsModal v-if=\"showUserDetails\" :user=\"selectedUser\" @close=\"closeUserDetails\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AddData from './AddData.vue';\r\nimport TableData from './TableData.vue';\r\nimport UpdateData from './UpdateData.vue';\r\nimport UserDetailsModal from './UserDetailsModal.vue'; // Import the modal component\r\nimport axios from 'axios';\r\nimport { API_ENDPOINT } from './apiConfig.js';\r\n\r\nexport default {\r\n  components: {\r\n    TableData,\r\n    AddData,\r\n    UpdateData,\r\n    UserDetailsModal,\r\n  },\r\n  data() {\r\n    return {\r\n      selectedUser: null,\r\n      users: [],\r\n      selectedUserId: null,\r\n      showUserDetails: false, // Flag to show/hide user details modal\r\n    };\r\n  },\r\n  created() {\r\n    this.fetchUsers();\r\n  },\r\n  methods: {\r\n    async fetchUsers() {\r\n      try {\r\n        const response = await axios.get(API_ENDPOINT);\r\n        this.users = response.data;\r\n      } catch (error) {\r\n        console.error('Error fetching users:', error);\r\n      }\r\n    },\r\n    async addUser(newUser) {\r\n      try {\r\n        const response = await axios.post(API_ENDPOINT, newUser);\r\n        this.users.push(response.data); \r\n        console.log('User added:', response.data);\r\n      } catch (error) {\r\n        console.error('Error adding user:', error);\r\n      }\r\n    },\r\n    async deleteUser(userId) {\r\n      try {\r\n        await axios.delete(`${API_ENDPOINT}/${userId}`);\r\n        this.users = this.users.filter(user => user.id !== userId);\r\n        console.log('User deleted with ID:', userId);\r\n      } catch (error) {\r\n        console.error('Error deleting user:', error);\r\n      }\r\n    },\r\n    editUser(userId) {\r\n      console.log('Editing user with ID:', userId);\r\n      this.selectedUserId = userId;\r\n      this.selectedUser = this.users.find(user => user.id === userId);\r\n    },\r\n    async updateUser(updatedUser) {\r\n      try {\r\n        const response = await axios.put(`${API_ENDPOINT}/${updatedUser.id}`, updatedUser);\r\n        const index = this.users.findIndex(user => user.id === updatedUser.id);\r\n        if (index !== -1) {\r\n          this.$set(this.users, index, response.data);\r\n        }\r\n        console.log('User updated:', response.data);\r\n      } catch (error) {\r\n        console.error('Error updating user:', error);\r\n      }\r\n      finally {\r\n        this.fetchUsers();\r\n      }\r\n    },\r\n    viewUser(userId) {\r\n      console.log('Viewing user with ID:', userId);\r\n      this.selectedUser = this.users.find(user => user.id === userId);\r\n      this.showUserDetails = true; // Set the flag to true to show the user details modal\r\n    },\r\n    closeUserDetails() {\r\n      this.showUserDetails = false; // Set the flag to false to hide the user details modal\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.page-container {\r\n  display:flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.create-button {\r\n    background-color: rgb(169, 59, 233); \r\n    color:white;\r\n    padding : 10px;\r\n    font-size: 18px;\r\n    margin: 20px;\r\n    border:none;\r\n    text-decoration: none;\r\n    border-radius: 8px;\r\n\r\n  }\r\n  .add {\r\n  margin-bottom: 20px;\r\n  }\r\n  .card {\r\n\r\n  box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2);\r\n  transition: 0.3s;\r\n}\r\n  </style>\r\n  "],"mappings":";AAYA,OAAOA,OAAM,MAAO,eAAe;AACnC,OAAOC,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,UAAS,MAAO,kBAAkB;AACzC,OAAOC,gBAAe,MAAO,wBAAwB,EAAE;AACvD,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,YAAW,QAAS,gBAAgB;AAE7C,eAAe;EACbC,UAAU,EAAE;IACVL,SAAS;IACTD,OAAO;IACPE,UAAU;IACVC;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,IAAI;MAClBC,KAAK,EAAE,EAAE;MACTC,cAAc,EAAE,IAAI;MACpBC,eAAe,EAAE,KAAK,CAAE;IAC1B,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMX,KAAK,CAACY,GAAG,CAACX,YAAY,CAAC;QAC9C,IAAI,CAACI,KAAI,GAAIM,QAAQ,CAACR,IAAI;MAC5B,EAAE,OAAOU,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C;IACF,CAAC;IACD,MAAME,OAAOA,CAACC,OAAO,EAAE;MACrB,IAAI;QACF,MAAML,QAAO,GAAI,MAAMX,KAAK,CAACiB,IAAI,CAAChB,YAAY,EAAEe,OAAO,CAAC;QACxD,IAAI,CAACX,KAAK,CAACa,IAAI,CAACP,QAAQ,CAACR,IAAI,CAAC;QAC9BW,OAAO,CAACK,GAAG,CAAC,aAAa,EAAER,QAAQ,CAACR,IAAI,CAAC;MAC3C,EAAE,OAAOU,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,oBAAoB,EAAEA,KAAK,CAAC;MAC5C;IACF,CAAC;IACD,